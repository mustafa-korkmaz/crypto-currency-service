    <link href="//fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,400italic" rel="stylesheet" type="text/css">
    <!-- ICONS-->
    <link rel="stylesheet" href="@Url.Content("~/Content/vendor/font-awesome/css/font-awesome.min.css")">
    <script src="@Url.Content("~/Content/vendor/jquery.browser/dist/jquery.browser.js")"></script>
    <script>
        if (window.jQBrowser.mobile && top.location != location) top.location.href = document.location.href;
    </script>
    <!-- VENDOR CSS-->
    <link rel="stylesheet" href="@Url.Content("~/Content/vendor/owl.carousel/dist/assets/owl.carousel.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/vendor/owl.carousel/dist/assets/owl.theme.default.css")">
