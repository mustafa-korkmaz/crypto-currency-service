<body ng-app="productServiceApp" ng-controller="ProductListController as ctrl">
    <!-- WEBSITE PRELOADER-->
    <div class="preloader">
        <div class="loading">Loading ...</div>
    </div>
    <!-- START HEADER-->
    <header id="header" class="header novideo">
        <div class="full-screen">
            <!-- START STICKY NAVBAR-->
            <div class="navbar navbar-default navbar-fixed-top navbar-sticky">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" data-toggle="collapse" data-target="#navbar-main" class="navbar-toggle">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a href="#" class="navbar-brand">
                            <img src="@Url.Content("~/Content/img/logo-dark.png")" alt="">
                        </a>
                    </div>
                    <!-- MENU-->
                    <div id="navbar-main" class="navbar-collapse collapse">
                        <ul class="nav navbar-nav navbar-right main-navbar">
                            <li>
                                <a href="#header">Anasayfa</a>
                            </li>
                            <li>
                                <a href="#features">Özellikler</a>
                            </li>
                            <li>
                                <a href="#feedbacks">Geri Bildirim</a>
                            </li>
                            <li>
                                <a href="#plans">Nedir</a>
                            </li>
                            <li>
                                <a href="#plans">Blog</a>
                            </li>
                            @*<li>
                                    <a href="#download">
                                        <strong>Download</strong>
                                    </a>
                                </li>*@
                        </ul>
                    </div>
                </div>
            </div>
            <!-- END STICKY NAVBAR-->
        <section>
            <div class="top-logo">
                <img src="@Url.Content("~/Content/img/logo-dark.png")" alt="">
            </div>
        <input class="fp-input" type="text" placeholder="marka,ürün" ng-model="ctrl.searchText" ng-keyup="ctrl.search()">
            <div class="col-lg-12 col-md-12 scrollbar" id="style-2" style="max-height: 250px; overflow-y: auto; background-color: white; border-radius: 4px" ng-show="ctrl.searchProducts.length > 0">
                <div class="form-group force-overflow">
                    <div class="media" ng-repeat="product in ctrl.searchProducts" style="padding: 10px; border-bottom: 1px solid #ccc">
                        <div class="media-left">
                            <a href="#">
                                <img class="media-object" width="72" height="72" ng-src="{{product.image_url}}" alt="...">
                            </a>
                        </div>
                        <div class="media-body text-left">
                            <h4 class="media-heading">{{product.name}}<span class="pull-right">{{product.company}}</span></h4>
                            <strong>{{product.price}}</strong> <small>TL</small>
                            <span class="pull-right">
                                <a href="#" class="btn">&nbsp; Takipçi <span class="badge alert-info pull-left">{{product.follower_total}} </span></a>

                                <a href="@Url.Action("Detail", "Product")?id={{product.id}}" class="btn btn-sm btn-default">Detay</a>
                            </span>

                        </div>
                    </div>
                    <span class="pull-right" ng-show="ctrl.searchProducts.length > 5">daha fazlası için <a href="@Url.Action("Index", "Product")?q={{ctrl.searchText}}"><b>tıklayınız</b></a></span>
                </div>
            </div>
