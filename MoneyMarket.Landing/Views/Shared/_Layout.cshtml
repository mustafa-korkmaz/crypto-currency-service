    <script src="@Url.Content("~/Content/vendor/nprogress/nprogress.js")"></script>
    <!-- FAVICON-->
    <link rel="icon" href="@Url.Content("~/Content/img/favicon/favicon.ico")">
    <link rel="apple-touch-icon" href="@Url.Content("~/Content/img/favicon/apple-touch-icon.png")">
